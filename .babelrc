{
  "presets" : [
  	["es2015"],
  	["env", {
      "targets": {
        "node": "5.10"
      }
    }]
  ],
  "plugins": [
    ["babel-plugin-root-import", 
    	[{
	      "rootPathPrefix": "~", // `~` is the default so you can remove this if you want
	      "rootPathSuffix": "source/js"
	    },
	    {
	      "rootPathPrefix": "@",
	      "rootPathSuffix": "source/data"
	    }]
    ]
  ]
}